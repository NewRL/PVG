<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="view_accr_medicine_food_Intolerance_tree" model="ir.ui.view">
            <field name="name">accr.medicine.food.Intolerance.tree</field>
            <field name="model">accr.medicine.food.Intolerance</field>
            <field name="priority" eval="8" />
            <field name="arch" type="xml">
                <tree string="Medicine Food Intolerance" editable="bottom">
                    <field name="name" string="Medicine Food Intolerance" />
                </tree>
            </field>
        </record>

        <record id="view_accr_medicine_food_Intolerance_form" model="ir.ui.view">
            <field name="name">accr.medicine.food.Intolerance.form</field>
            <field name="model">accr.medicine.food.Intolerance</field>
            <field name="priority" eval="8" />
            <field name="arch" type="xml">
                <form string="Medicine Food Intolerance">
                    <sheet>
                        <group col="4">
                            <field name="name" string="Medicine Food Intolerance" />
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="view_accr_medicine_food_Intolerance_search" model="ir.ui.view">
            <field name="name">accr.medicine.food.Intolerance.search</field>
            <field name="model">accr.medicine.food.Intolerance</field>
            <field name="priority" eval="8" />
            <field name="arch" type="xml">
                <search string="Medicine Food Intolerance">
                    <field name="name" select="True" string="Medicine Food Intolerance"/>
                </search>
            </field>
        </record>

        <record model="ir.actions.act_window" id="act_open_medicine_food_Intolerance_view">
            <field name="name">Medicine Food Intolerance</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">accr.medicine.food.Intolerance</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="search_view_id" ref="view_accr_medicine_food_Intolerance_search"/>
            <field name="domain">[]</field>
            <field name="context">{}</field>
        </record>

        <record model="ir.actions.act_window.view" id="act_open_medicine_food_Intolerance_view_form">
            <field name="act_window_id" ref="act_open_medicine_food_Intolerance_view" />
            <field name="sequence" eval="20" />
            <field name="view_mode">form</field>
            <field name="view_id" ref="view_accr_medicine_food_Intolerance_form" />
        </record>

        <record model="ir.actions.act_window.view" id="act_open_medicine_food_Intolerance_view_tree">
            <field name="act_window_id" ref="act_open_medicine_food_Intolerance_view" />
            <field name="sequence" eval="10" />
            <field name="view_mode">tree</field>
            <field name="view_id" ref="view_accr_medicine_food_Intolerance_tree" />
        </record>

    </data>
</odoo>
                            <t t-set="fat_percentage" t-value="0" />
                            <t t-set="mucles_kg" t-value="0.0" />
                            <t t-set="lean_muscles" t-value="0" />
                            <t t-set="fat_control_kg" t-value="0.0" />
                            <t t-set="muscles_control_kg" t-value="0.0" />
                            <t t-set="icw" t-value="0" />
                            <t t-set="ecw" t-value="0" />
                            <script type="text/javascript">
                                graph_records_height = [];
                                graph_records_weight = [];
                                graph_records_fats = [];
                                graph_records_muscles = [];
                                graph_records_fats_muscles_control = [];
                                graph_records_icw_ecw = [];
                                <t t-foreach="doc.bca.sorted(key=lambda b: b.bca_date)" t-as="bca">
                                    <t t-set="bca_report_name" t-value="bca.bca_date"/>
                                    bca_report_name = '<t t-esc="bca_report_name"/>';
                                    <t t-set="height" t-value="bca.height"/>
                                    height = <t t-esc="height" /> ;
                                    <t t-set="ideal_weight" t-value="bca.ideal_weight" />
                                    ideal_weight = <t t-esc="ideal_weight" />;
                                    <t t-set="actual_weight" t-value="bca.actual_weight" />
                                    actual_weight = <t t-esc="actual_weight" />;
                                    <t t-set="bmr" t-value="bca.bmr.display_name" />
                                    <t t-set="fat_kg" t-value="bca.fat_kg" />
                                    fat_kg = <t t-esc="fat_kg" />;
                                    <t t-set="fat_percentage" t-value="bca.fat_percentage" />
                                    fat_percentage = <t t-esc="fat_percentage" />;
                                    <t t-set="mucles_kg" t-value="bca.mucles_kg" />
                                    mucles_kg = <t t-esc="mucles_kg" />;
                                    <t t-set="lean_muscles" t-value="bca.lean_muscles" />
                                    lean_muscles = <t t-esc="lean_muscles" />;
                                    <t t-set="fat_control_kg" t-value="bca.fat_control_kg" />
                                    fat_control_kg = <t t-esc="fat_control_kg" />;
                                    <t t-set="muscles_control_kg" t-value="bca.muscles_control_kg" />
                                    muscles_control_kg = <t t-esc="muscles_control_kg" />;
                                    <t t-set="icw" t-value="bca.icw" />
                                    icw = <t t-esc="icw" />;
                                    <t t-set="ecw" t-value="bca.ecw" />
                                    ecw = <t t-esc="ecw" />;

                                    graph_records_height.push({key: bca_report_name, values: [{"label": "Height", "value": height}]});

                                    graph_records_weight.push({key: bca_report_name, values: [{"label": "Ideal Weight", "value": ideal_weight}, 
                                    {"label": "Actual Weight", "value": actual_weight}]});

                                    graph_records_fats.push({key: bca_report_name, values: [{"label": "Fat.kg", "value": fat_kg}, {"label": "Fat./%", "value": fat_percentage}]});

                                    graph_records_muscles.push({key: bca_report_name, values:[{"label": "Mucles.kg", "value": mucles_kg}, {"label": "Lean Muscles", "value": lean_muscles}]});

                                    graph_records_fats_muscles_control.push({key: bca_report_name, values:[{"label": "FAT Control.kg", "value": fat_control_kg}, 
                                    {"label": "Muscles Control.kg", "value": muscles_control_kg}]});

                                    graph_records_icw_ecw.push({key: bca_report_name, values:[{"label": "ICW", "value": icw}, {"label": "ECW", "value": ecw}]});

                                </t>

                                    nv.addGraph(function () {
                                    var chart_height = nv.models.multiBarChart()
                                        .x(function(d) { return d.label })    //Specify the data accessors.
                                        .y(function(d) { return d.value })
                                        .showControls(false)
                                        .rotateLabels(30)
                                        .reduceXTicks(false)
                                        .groupSpacing(0.3)
                                        ;
                                        
                                        d3.select('#chart_height svg')
                                            .datum(graph_records_height)
                                            .call(chart_height);

                                        nv.utils.windowResize(chart_height.update);

                                        return chart_height;
                                    });

                                    nv.addGraph(function () {
                                    var chart_weight = nv.models.multiBarChart()
                                        .x(function(d) { return d.label })    //Specify the data accessors.
                                        .y(function(d) { return d.value })
                                        .showControls(false)
                                        .rotateLabels(30)
                                        .reduceXTicks(false)
                                        .groupSpacing(0.3)
                                        ;
                                        
                                        d3.select('#chart_weight svg')
                                            .datum(graph_records_weight)
                                            .call(chart_weight);

                                        nv.utils.windowResize(chart_weight.update);

                                        return chart_weight;
                                    });

                                    nv.addGraph(function () {
                                    var chart_fats = nv.models.multiBarChart()
                                        .x(function(d) { return d.label })    //Specify the data accessors.
                                        .y(function(d) { return d.value })
                                        .showControls(false)
                                        .rotateLabels(30)
                                        .reduceXTicks(false)
                                        .groupSpacing(0.3)
                                        ;
                                        
                                        d3.select('#chart_fats svg')
                                            .datum(graph_records_fats)
                                            .call(chart_fats);

                                        nv.utils.windowResize(chart_fats.update);

                                        return chart_fats;
                                    });

                                    nv.addGraph(function () {
                                    var chart_muscles = nv.models.multiBarChart()
                                        .x(function(d) { return d.label })    //Specify the data accessors.
                                        .y(function(d) { return d.value })
                                        .showControls(false)
                                        .rotateLabels(30)
                                        .reduceXTicks(false)
                                        .groupSpacing(0.3)
                                        ;
                                        
                                        d3.select('#chart_muscles svg')
                                            .datum(graph_records_muscles)
                                            .call(chart_muscles);

                                        nv.utils.windowResize(chart_muscles.update);

                                        return chart_muscles;
                                    });

                                    nv.addGraph(function () {
                                    var chart_fats_muscles_control = nv.models.multiBarChart()
                                        .x(function(d) { return d.label })    //Specify the data accessors.
                                        .y(function(d) { return d.value })
                                        .showControls(false)
                                        .rotateLabels(30)
                                        .reduceXTicks(false)
                                        .groupSpacing(0.3)
                                        ;
                                        
                                        d3.select('#chart_fats_muscles_control svg')
                                            .datum(graph_records_fats_muscles_control)
                                            .call(chart_fats_muscles_control);

                                        nv.utils.windowResize(chart_fats_muscles_control.update);

                                        return chart_fats_muscles_control;
                                    });

                                    nv.addGraph(function () {
                                    var chart_icw_ecw = nv.models.multiBarChart()
                                        .x(function(d) { return d.label })    //Specify the data accessors.
                                        .y(function(d) { return d.value })
                                        .showControls(false)
                                        .rotateLabels(30)
                                        .reduceXTicks(false)
                                        .groupSpacing(0.3)
                                        ;
                                        
                                        d3.select('#chart_icw_ecw svg')
                                            .datum(graph_records_icw_ecw)
                                            .call(chart_icw_ecw);

                                        nv.utils.windowResize(chart_icw_ecw.update);

                                        return chart_icw_ecw;
                                    });


                            </script>
                            <div class="row">
                                <div class="col-4">
